DATABASE_URL=postgresql://user:password@host/database?sslmode=require

SPOTIFY_CLIENT_ID=your_client_id
SPOTIFY_CLIENT_SECRET=your_client_secret

# Encryption (32 bytes = 64 hex characters)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
ENCRYPTION_KEY=
SESSION_SECRET=
CRON_SECRET=
# Backend
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000

# Redis (Aiven Cloud)
REDIS_URL=rediss://default:password@redis-project.aivencloud.com:12345

# Circuit Breaker (optional - they have defaults)
# SPOTIFY_CB_THRESHOLD=5
# SPOTIFY_CB_RESET_TIMEOUT=30000
# SPOTIFY_CB_WINDOW_DURATION=60000
